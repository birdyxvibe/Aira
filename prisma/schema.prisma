generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id            String      @map("_id") @id @default(auto()) @db.ObjectId
  createdAt     DateTime    @default(now())
  
  discordId     String      @unique

  Coins         Int         @default(0)
}