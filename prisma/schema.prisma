generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            				Int         @id @default(autoincrement())
  CreatedAt     				DateTime    @default(now())
  
  DiscordId     				String      @unique

  Coins         				Int         @default(0)

	DailyIncomeNext				DateTime		@default(now())
	DailyStreak						Int					@default(0)
}